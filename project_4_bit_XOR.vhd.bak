library IEEE;
use IEEE.STD_LOGIC_1164.ALL;
entity FBXOR is
    Port ( A : in  STD_LOGIC_VECTOR (3 downto 0); 
           B : in  STD_LOGIC_VECTOR (3 downto 0);
           Z : out  STD_LOGIC_VECTOR (3 downto 0));
end FBXOR;

architecture Behavioral of FBXOR is

begin
Z(0)<= A(0) XOR B(0);
Z(1)<= A(1) XOR B(1);
Z(2)<= A(2) XOR B(2);
Z(3)<= A(3) XOR B(3);

end Behavioral;